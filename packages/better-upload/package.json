{
  "name": "better-upload",
  "description": "Simple file uploads for React",
  "version": "0.2.0",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./server": {
      "import": "./dist/server/index.mjs",
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./server/helpers": {
      "import": "./dist/server-helpers/index.mjs",
      "types": "./dist/server-helpers/index.d.ts",
      "default": "./dist/server-helpers/index.js"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    }
  },
  "dependencies": {
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "*",
    "react": "*"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@types/node": "^22.7.0",
    "@types/react": "^18",
    "react": "^18",
    "tsup": "^8.4.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "next",
    "react",
    "nextjs",
    "remix",
    "upload",
    "easy uploads",
    "simple",
    "files",
    "file upload"
  ],
  "license": "MIT",
  "homepage": "https://better-upload.js.org",
  "bugs": {
    "url": "https://github.com/Nic13Gamer/better-upload/issues"
  },
  "author": "Nicholas",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nic13Gamer/better-upload.git"
  }
}
